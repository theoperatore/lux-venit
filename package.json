{
  "name": "lunina",
  "version": "0.1.0",
  "description": "the technical workings for the \"Lunina\" piece for the Synaesthesia Playground.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf public/bundle.js",
    "js": "browserify ./ui/index.js | uglifyjs -cm -o public/bundle.js",
    "js:watch": "watchify ./ui/index.js -o public/bundle.js -v",
    "js:prod": "NODE_ENV=production npm run js",
    "dev:server": "node server.js",
    "start": "npm run js:prod && forever server.js"
  },
  "engines": {
    "npm": ">=3.7.3",
    "node": ">=5.8.0"
  },
  "author": "Alex Petersen <theoperatore@gmail.com> (http://anpetersen.me)",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.6.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "forever": "^0.15.1",
    "node-osc": "^2.0.3",
    "socket.io": "^1.4.5"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "localenvify": "^1.0.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "redux": "^3.3.1",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "localenvify"
    ]
  }
}
